<h1> All Voulenteer Events </h1>

<div>
  {{#each eventList as |event|}}
  <div class="event-details">
    <img src="/images/{{event.picture}}" alt="{{event.name}}">
    <h2>{{event.name}}</h2>
    <p>{{event.description}}</p>
    <p>Start Date: {{event.date_start}}</p>
    <p>End Date: {{event.date_end}}</p>
  </div>
  {{/each }}
</div>