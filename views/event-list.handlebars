 <h1 id="eventDetailsText"> All Volunteer Events </h1>
 
 <div id="eventListDiv">
 {{#each eventsList as |event|}}
    <div class="event-details">
  <img class="FullListImgs" src="/images/{{event.picture}}" alt="{{event.name}}">
  <div id="eventDetailsText">
  <h2>{{event.name}}</h2>
  <p>{{event.description}}</p>
  <p>Start Date: {{event.date_start}}</p>
  <p>End Date: {{event.date_end}}</p>
  </div>
</div>
    {{/each }}
    </div>